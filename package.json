{
  "name": "@mdude2314/backstage-plugin-scaffolder-npm-actions",
  "description": "The npm-actions module for @backstage/plugin-scaffolder-backend",
  "version": "1.2.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/arhill05/backstage-plugin-scaffolder-npm-actions.git"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./new-backend": "./src/new-backend.ts"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ],
      "new-backend": [
        "src/new-backend.ts"
      ]
    }
  },
  "backstage": {
    "role": "backend-plugin-module"
  },
  "scripts": {
    "start": "backstage-cli package start",
    "build": "backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack"
  },
  "dependencies": {
    "@backstage/plugin-scaffolder-backend": "^1.25.0"
  },
  "devDependencies": {
    "@backstage/backend-common": "^0.25.0",
    "@backstage/cli": "^0.27.1",
    "@types/jest": "^29.5.13"
  },
  "files": [
    "dist"
  ]
}
